From 04feb19f1929c9cedb7b8c78fb8a98716662b2b9 Mon Sep 17 00:00:00 2001
From: Winston Jašaragić <wulsic@gmail.com>
Date: Thu, 8 Dec 2016 11:55:33 +0100
Subject: [PATCH] Allow changing global flags

Change-Id: Ib66d83e618c91902982db3bce85bd978bd52195d
---
 core/config.mk | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/core/config.mk b/core/config.mk
index 6a79d3e..c7d142a 100644
--- a/core/config.mk
+++ b/core/config.mk
@@ -667,13 +667,13 @@ include vendor/cm/build/core/mtk_target.mk
 # Set up final options.
 # ###############################################################
 
-ifneq ($(COMMON_GLOBAL_CFLAGS)$(COMMON_GLOBAL_CPPFLAGS),)
-$(warning COMMON_GLOBAL_C(PP)FLAGS changed)
-$(info *** Device configurations are no longer allowed to change the global flags.)
-$(info *** COMMON_GLOBAL_CFLAGS: $(COMMON_GLOBAL_CFLAGS))
-$(info *** COMMON_GLOBAL_CPPFLAGS: $(COMMON_GLOBAL_CPPFLAGS))
-$(error bailing...)
-endif
+#ifneq ($(COMMON_GLOBAL_CFLAGS)$(COMMON_GLOBAL_CPPFLAGS),)
+#$(warning COMMON_GLOBAL_C(PP)FLAGS changed)
+#$(info *** Device configurations are no longer allowed to change the global flags.)
+#$(info *** COMMON_GLOBAL_CFLAGS: $(COMMON_GLOBAL_CFLAGS))
+#$(info *** COMMON_GLOBAL_CPPFLAGS: $(COMMON_GLOBAL_CPPFLAGS))
+#$(error bailing...)
+#endif
 
 # These can be changed to modify both host and device modules.
 COMMON_GLOBAL_CFLAGS:= -DANDROID -fmessage-length=0 -W -Wall -Wno-unused -Winit-self -Wpointer-arith
-- 
2.7.4

